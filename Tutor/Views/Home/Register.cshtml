<script>
    function validateForm() {
        if (document.getElementById("photoFile").files.length > 0) {
            var size = document.getElementById("photoFile").files.item(0).size;

            if (size > (1024000 * 3)) {
                alert("上傳檔案請小於 3M");
                return false;
            }
            else {
                alert("繳費單已寄送至您的註冊電子信箱 p.s.此費用為一次性會員開通費");
                return true;
            }
        }
        else {
            alert("繳費單已寄送至您的註冊電子信箱 p.s.此費用為一次性會員開通費");
            return true;
        }
        

    }

    function format_float(num, pos) {
        var size = Math.pow(10, pos);
        return Math.round(num * size) / size;
    }

    function preview(input) {

        if (input.files && input.files[0]) {
            var reader = new FileReader();

            reader.onload = function (e) {
                $("#photoimg").show();
                $('.photoimg').attr('src', e.target.result);
                var KB = format_float(e.total / 1024, 2);
                $('.size').text("檔案大小：" + KB + " KB");
            }

            reader.readAsDataURL(input.files[0]);
        }
    }

    $("body").on("change", ".photoFile", function () {
        preview(this);
    })
    var model = @Html.Raw(Json.Encode(Model));

    var windowFlag = true;
    $(document).ready(function () {
        BindView(model);
        if ("@Model.photoName" == "")
            $("#photoimg").hide();
    });

    function checkRecommendCode() {

        var SuggestCode = $("#SuggestCode").val();
        if (SuggestCode == "")
            return
        $.ajax({
            url: '@Url.Action("checkRecommendCode", "Home")',
            type: 'post',
            data: { Code: SuggestCode },
            success: function (result) {
                if (result != "") {
                    alert(result);
                    $("#SuggestCode").val("");
                }

            },
            error: function () {
            }
        });
    }
    function paddingLeft(str, lenght) {
        if (str.length >= lenght)
            return str;
        else
            return paddingLeft("0" + str, lenght);
    }
</script>

